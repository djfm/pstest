{
    "name": "prestashop/pstest",
    "require": {
        "phpunit/phpunit": "~4.5",
        "symfony/console": "~2.6",
        "symfony/finder": "~2.6",
        "facebook/webdriver": "0.5.*",
        "djfm/socket-rpc": "~1",
        "evenement/evenement": "~2.0",
        "symfony/dom-crawler": "~2.6",
        "ext-imap": "*",
        "ext-curl": "*"
    },
    "authors": [
        {
            "name": "fmdj",
            "email": "fmdj@prestashop.com"
        }
    ],
    "autoload": {
        "classmap": [
            "src/ioc/"
