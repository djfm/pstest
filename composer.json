            "PrestaShop\\Selenium\\Tests\\": "tests/selenium/",
            "PrestaShop\\FileSystem\\": "src/fs/",
            "PrestaShop\\FileSystem\\Tests\\": "tests/fs/",
            "PrestaShop\\Proc\\": "src/proc/",
            "PrestaShop\\Proc\\Tests\\": "tests/proc/",
            "PrestaShop\\TestRunner\\": "src/test-runner/",
            "PrestaShop\\TestRunner\\Tests\\": "tests/test-runner/",
            "PrestaShop\\TestRunner\\Tests\\Fixtures\\": "tests/test-runner/fixtures/",
            "PrestaShop\\IoC\\Tests\\": "tests/ioc/",
            "PrestaShop\\IoC\\Tests\\Fixtures\\": "tests/ioc/fixtures/",
            "PrestaShop\\PrestaShop\\FunctionalTest\\": "FunctionalTest",
            "PrestaShop\\PrestaShop\\CloudTest\\": "CloudTest"
        }
    }
}
